{
	"name": "confusion",
	"version": "1.0.0",
	"description": "This is a website for Ristorante Con Fusion",
	"main": "index.html",
	"scripts": {
		"start": "npm run dev",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lite": "lite-server",
		"scss": "node-sass -o resources/css/ resources/css/",
		"watch:scss": "onchange \"resources/css/*.scss\" -- npm run scss",
		"watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
		"dev": "npm-run-all -p watch:scss lite",
		"clean": "rimraf dist",
		"copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
		"imagemin": "imagemin img/* --out-dir='dist/img'",
		"usemin": "usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html && usemin menu.html -d dist --htmlmin -o dist/menu.html",
		"build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"cssmin": "^0.4.3",
		"htmlmin": "0.0.7",
		"lite-server": "^2.6.1",
		"node-sass": "^7.0.0",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"parallelshell": "^3.0.1",
		"rimraf": "^3.0.2",
		"uglify-js": "^3.12.0",
		"usemin-cli": "^0.6.0"
	},
	"dependencies": {
		"bootstrap": "^4.5.3",
		"bootstrap-social": "^5.1.1",
		"font-awesome": "^4.7.0",
		"jquery": "^3.5.1",
		"popper.js": "^1.16.1"
	}
}
